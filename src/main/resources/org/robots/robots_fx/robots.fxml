<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="pane" prefHeight="720.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.robots.robots_fx.RobotsController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="instructionText" text="Click Start button to launch robots" AnchorPane.leftAnchor="0.0" />
   <Slider fx:id="FPSSlider" blockIncrement="1.0" layoutX="80.0" layoutY="45.0" max="30.0" min="1.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="9.0" prefWidth="600.0" value="1.0" AnchorPane.rightAnchor="101.0" AnchorPane.topAnchor="0.0" />
    <Button fx:id="button" onAction="#onButtonClick" text="Start" AnchorPane.rightAnchor="0.0" />
</AnchorPane>
