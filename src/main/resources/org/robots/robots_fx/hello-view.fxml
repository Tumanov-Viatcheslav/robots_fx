<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane fx:id="pane" xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.robots.robots_fx.RobotsController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="instructionText" text="Click Start button to launch robots"/>
    <Button fx:id="button" AnchorPane.rightAnchor="0.0" text="Start" onAction="#onButtonClick"/>
</AnchorPane>
